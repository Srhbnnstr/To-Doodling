<div class="col-xs-offset-1 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-1 col-lg-8 col-lg-offset-1" style="padding-left:0px">
  <div class="container-fluid">
    <h1>Incomplete Tasks<%= image_tag("pencil-gif.gif", class: "img-responsive") %></h1>
  </div>
</div>
<div class="col-xs-offset-1 col-sm-10 col-sm-offset-1 col-md-10 col-lg-10" style="padding-left:0px">
  <div class="todos container-fluid">
    <% if current_user.todos.empty? %>
        <p>You haven't created any tasks yet.</p>
    <% else %>
    <% current_user.todos.each do |t| %>
      <li>
        <input type="checkbox" id="cbox1">
        <label class="todoItem"><%= "#{t.title}" %></label><hr>
      </li>
      <% end %>
  <% end %>
<%= button_to 'My Lists', lists_path(@todo), :method => :get, class: "btn btn-small btn-primary" %>
  </div>
</div>
