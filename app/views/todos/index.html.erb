<div class="col-xs-offset-1 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-1 col-lg-8 col-lg-offset-1" style="padding-left:0px">
  <div class="container-fluid">
    <h1>Incomplete Tasks<%= image_tag("pencil-gif.gif", class: "img-responsive") %></h1>
  </div>
</div>
<div class="col-xs-offset-3 col-sm-6 col-sm-offset-3 col-md-8 col-md-offset-3 col-lg-8 col-lg-offset-3" style="padding-left:0px">
  <div class="todos row">
    <% current_user.todos.each do |t| %><hr>
    <p><%= link_to "#{t.title}", list_path(t)%></p>
    <% end %>
  </div>
</div>
