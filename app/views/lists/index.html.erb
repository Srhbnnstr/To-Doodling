<div class="col-xs-offset-1 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-1 col-lg-8 col-lg-offset-1" style="padding-left:0px">
  <div class="container-fluid">
    <h1>My Lists</h1>
  </div>
</div>
<br><br>
<div class="col-sm-offset-1 col-md-offset-1 col-lg-6">
  <div class="lists container">
    <% if @lists.empty? %>
        <p>You haven't created any lists yet.</p>
    <% else %>
      <% @lists.each do |t|%>
        <%= link_to "#{t.list_title}", list_path(t) %><hr>
      <% end %>
    <% end %>
    <%= button_to "Add List", new_list_path, :method => "get", class: "btn btn-small btn-primary" %>
  </div>
</div>
