<h1><%= @list.list_title %></h1>
<% if current_user%>
  <%= button_to "Edit", edit_list_path(@list), :method => "get", class: "btn btn-small btn-success"%></div>
  <%= button_to "Delete List", (@list), data: {:confirm=>'Are you sure you want to delete this list?'}, :method => "destroy", class: "btn btn-small btn-danger" %></div>
  <%= button_to "Attend", new_list_path(@list), :method => "get", class: "btn btn-small btn-primary" %></div>
<% end %>
